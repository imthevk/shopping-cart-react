{
  "homepage": "https://rawfour.github.io/shopping-cart-react/",
  "name": "shopping-cart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/styles": "^4.9.13",
    "@testing-library/jest-dom": "^4.2.4",
